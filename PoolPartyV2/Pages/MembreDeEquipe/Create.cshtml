@page
@model PoolPartyV2.Pages.MembreDeEquipe.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Ajouter</h1>

<h4>MembreEquipe</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MembreEquipe.Equipe.ID" value="@Model.Equipe.ID" />
            <div class="form-group">
                <label asp-for="MembreEquipe.IDLicencie" class="control-label"></label>
                <select asp-for="MembreEquipe.IDLicencie" class="form-control" asp-items="ViewBag.IDLicencie"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Retour</a>
</div>

